void setup() {
  arduboy.begin();
  arduboy.setFrameRate(60);
}

void loop() {
  if (!(arduboy.nextFrame()))
    return;
  arduboy.clear();

 switch (gameProgress)
  {
case GAME_INTRO:
      arduboy.drawBitmap(0, 0, titlescreen, 128, 64, WHITE);
      arduboy.setCursor(15, 56);
      arduboy.print("by @JetJaguar3030");
      arduboy.display();
      arduboy.delayShort(3000);
      gameProgress = GAME;
      break;

case GAME:
  if(arduboy.pressed(UP_BUTTON)) {
   arduboy.drawBitmap(0, 0, UPBUTTON, 128, 64, WHITE); 
   sound.tones(note);
  }
  else if(arduboy.pressed(DOWN_BUTTON)){
  arduboy.drawBitmap(0, 0, DOWNBUTTON, 128, 64, WHITE); 
  sound.tones(note);
  }
  else if(arduboy.pressed(RIGHT_BUTTON)){
   arduboy.drawBitmap(0, 0, RBUTTON, 128, 64, WHITE); 
   sound.tones(note);
  }
  else if(arduboy.pressed(LEFT_BUTTON)){
   arduboy.drawBitmap(0, 0, LBUTTON, 128, 64, WHITE); 
   sound.tones(note);
  }
  else if(arduboy.pressed(A_BUTTON)){
  arduboy.drawBitmap(0, 0, ABUTTON, 128, 64, WHITE); 
  sound.tones(note);
  }
  else if(arduboy.pressed(B_BUTTON)){
  arduboy.drawBitmap(0, 0, BBUTTON, 128, 64, WHITE); 
  sound.tones(note);
  }else{
  arduboy.drawBitmap(0, 0, rested, 128, 64, WHITE); 
  sound.tones(note);
  }
  break;
  }
   
  arduboy.display();

}
